# Example Terraform variables file
# Copy this to terraform.tfvars and update with your actual values

# AWS Region
region = "us-east-1"

# Account IDs
database_account_id    = "111111111111"  # Replace with your database account ID
application_account_id = "222222222222"  # Replace with your application account ID

# VPC IDs
database_vpc_id    = "vpc-0123456789abcdef0"  # Replace with your database VPC ID
application_vpc_id = "vpc-0987654321fedcba0"  # Replace with your application VPC ID

# Database subnet IDs (where RDS instances are located)
database_subnet_ids = [
  "subnet-0123456789abcdef0",  # Replace with actual subnet IDs
  "subnet-0987654321fedcba0"
]
